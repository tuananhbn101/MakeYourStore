<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.makeyourstore">

    <application
        android:name="object_App.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MakeYourStore">
        <activity android:name="Activity.Add_Product"></activity>
        <activity android:name="Activity.List_Image_Choose"></activity>
        <activity android:name="Activity.Change_Password"></activity>
        <activity android:name="login.Login_Activity">
        </activity>
        <activity android:name="Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>